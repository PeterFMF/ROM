     RowBox[{
      RowBox[{"{", 
       RowBox[{"x2", ",", " ", "y2"}], "}"}], "=", "BB"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"k", " ", "=", " ", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"y2", " ", "-", " ", "y1"}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"x2", " ", "-", " ", "x1"}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"n", " ", "=", 
      RowBox[{"n", " ", "/.", 
       RowBox[{"First", "[", 
        RowBox[{"Solve", "[", 
