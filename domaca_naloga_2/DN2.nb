Cell[BoxData[
 RowBox[{
  RowBox[{"Presek", "[", 
   RowBox[{
    RowBox[{"Daljica", "[", 
     RowBox[{"AA_", ",", "BB_"}], "]"}], ",", 
    RowBox[{"Daljica", "[", 
     RowBox[{"CC_", ",", "DD_"}], "]"}]}], "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "resitev", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"resitev", " ", "=", " ", 
      RowBox[{"Solve", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{
          RowBox[{
           RowBox[{"AA", " ", "+", " ", 
            RowBox[{"r", 
             RowBox[{"(", 
              RowBox[{"BB", " ", "-", " ", "AA"}], ")"}]}]}], " ", "\[Equal]",
            " ", 
           RowBox[{"CC", " ", "+", " ", 
            RowBox[{"s", 
             RowBox[{"(", 
              RowBox[{"DD", " ", "-", " ", "CC"}], ")"}]}]}]}], ",", " ", 
          RowBox[{"r", " ", "\[GreaterEqual]", " ", "0"}], ",", " ", 
          RowBox[{"r", " ", "\[LessEqual]", " ", "1"}], ",", " ", 
          RowBox[{"s", " ", "\[GreaterEqual]", " ", "0"}], ",", " ", 
          RowBox[{"s", " ", "\[LessEqual]", " ", "1"}]}], "}"}], ",", " ", 
        RowBox[{"{", 
         RowBox[{"r", ",", " ", "s"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"resitev", " ", "\[Equal]", " ", 
        RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"First", "[", 
        RowBox[{
         RowBox[{"AA", " ", "+", " ", 
          RowBox[{"r", 
           RowBox[{"(", 
            RowBox[{"BB", " ", "-", " ", "AA"}], ")"}]}]}], " ", "/.", " ", 
         "resitev"}], "]"}]}], "\[IndentingNewLine]", "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
